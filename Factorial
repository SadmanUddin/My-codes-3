using System;
namespace sadman
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int a = Convert.ToInt32(Console.ReadLine());

            int b = 1;

            for(int i=1;i<=a;i++)
            {
                b=(b*i);
            }
            Console.WriteLine(b);
        }
    }
}
